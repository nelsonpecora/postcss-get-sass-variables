{
  "name": "postcss-get-sass-variables",
  "version": "1.1.0",
  "description": "PostCSS plugin to get an object containing SASS variables and their values",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint --max-warnings 0 index.js",
    "test-with-coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha",
    "test": "npm run lint && npm run test-with-coverage",
    "coveralls": "npm run test-with-coverage -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nelsonpecora/postcss-get-sass-variables.git"
  },
  "keywords": [
    "postcss-plugin",
    "sass",
    "variables"
  ],
  "author": "Nelson Pecora",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nelsonpecora/postcss-get-sass-variables/issues"
  },
  "homepage": "https://github.com/nelsonpecora/postcss-get-sass-variables#readme",
  "dependencies": {
    "postcss": "^6.0.2"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "coveralls": "^2.13.1",
    "eslint": "^5.14.0",
    "istanbul": "^0.4.5",
    "lodash": "^4.17.4",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
